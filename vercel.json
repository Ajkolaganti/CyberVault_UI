{
  "rewrites": [
    {
      "source": "/api/v1/accounts",
      "destination": "https://cybervault-api-a1fo.onrender.com/api/v1/accounts"
    },
    {
      "source": "/api/v1/accounts/:id",
      "destination": "https://cybervault-api-a1fo.onrender.com/api/v1/accounts/:id"
    },
    {
      "source": "/api/v1/accounts/:id/rotate",
      "destination": "https://cybervault-api-a1fo.onrender.com/api/v1/accounts/:id/rotate"
    },
    {
      "source": "/api/v1/accounts/statistics",
      "destination": "https://cybervault-api-a1fo.onrender.com/api/v1/accounts/statistics"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://cybervault-api-a1fo.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, PATCH, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Origin, X-Requested-With, Content-Type, Accept, Authorization, X-Supabase-Token, Cache-Control"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "86400"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "vite"
}
